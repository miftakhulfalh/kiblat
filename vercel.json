{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/kiblat.js"
    }
  ],
  "crons": [
    {
      "path": "/api/kiblat?rashdul=1",
      "schedule": "50 9 3 5 *"
    },
    {
      "path": "/api/kiblat?rashdul=1",
      "schedule": "18 9 27 5 *"
    },
    {
      "path": "/api/kiblat?rashdul=1",
      "schedule": "18 9 28 5 *"
    },
    {
      "path": "/api/kiblat?rashdul=1",
      "schedule": "27 9 15 7 *"
    },
    {
      "path": "/api/kiblat?rashdul=1",
      "schedule": "27 9 16 7 *"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
