{
  "version": 2,
  "builds": [
    {
      "src": "api/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/kiblat.js"
    }
  ],
  "crons": [
    {
      "path": "/api/rashdul",
      "schedule": "42 5 3 5 *"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
